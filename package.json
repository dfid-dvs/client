{
    "name": "dfid",
    "version": "1.0.0",
    "description": "DFID Data Visualization System",
    "main": "src/index.tsx",
    "keywords": [],
    "author": "",
    "license": "ISC",
    "sideEffects": [
        "./node_modules/mapbox-gl/dist/mapbox-gl.css",
        "./src/Root/styles.css"
    ],
    "scripts": {
        "start": "webpack serve --mode development --config config/webpack.dev.ts --history-api-fallback",
        "build": "webpack --mode production --config config/webpack.prod.ts",
        "build-dev": "webpack --mode development --config config/webpack.dev.ts",
        "serve": "http-server ./build/ --proxy http://localhost:8080? -g -c-1",
        "test": "jest",
        "test:watch": "jest --watch",
        "typecheck": "tsc",
        "lint": "eslint ./src",
        "lint-fix": "eslint ./src --fix",
        "test:coverage": "jest --coverage",
        "deploy:dev": "test -d build && cp -f build/index.html build/200.html && surge --domain dvs-nepal.surge.sh build/"
    },
    "jest": {
        "setupFiles": [
            "./src/utils/setupTests.js"
        ],
        "moduleNameMapper": {
            "^.+\\.(css|scss|svg|gif|jpg|png)$": "identity-obj-proxy"
        },
        "moduleFileExtensions": [
            "ts",
            "tsx",
            "js",
            "jsx"
        ],
        "globals": {
            "NODE_ENV": "test"
        },
        "moduleDirectories": [
            "node_modules",
            "src"
        ],
        "testMatch": [
            "<rootDir>/src/**/__tests__/**/*.js?(x)",
            "<rootDir>/src/**/?(*.)(spec|test).js?(x)",
            "<rootDir>/src/**/__tests__/**/*.ts?(x)",
            "<rootDir>/src/**/?(*.)(spec|test).ts?(x)"
        ],
        "testPathIgnorePatterns": [
            "<rootDir>/src/vendor/"
        ],
        "testEnvironment": "jsdom",
        "testURL": "http://localhost",
        "transform": {
            "^.+\\.(ts|tsx)$": "babel-jest",
            "^.+\\.(js|jsx)$": "babel-jest"
        },
        "transformIgnorePatterns": [
            "[/\\\\]node_modules[/\\\\].+\\.(ts|tsx)$",
            "[/\\\\]node_modules[/\\\\].+\\.(js|jsx)$"
        ]
    },
    "devDependencies": {
